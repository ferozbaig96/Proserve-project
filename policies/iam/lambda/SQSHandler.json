{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "logs:CreateLogGroup",
                "logs:CreateLogStream",
                "logs:PutLogEvents",
                "ssm:Get*",
                "ssm:List*",
                "kms:Decrypt",
                "sqs:ReceiveMessage",
                "sqs:DeleteMessage",
                "sqs:GetQueueAttributes",
                "secretsmanager:GetSecretValue",
                "rds-data:ExecuteStatement"
            ],
            "Resource": "*"
        }
    ]
}
